          "choices_helper": "scheming_vocabulary_service_choices",
          "form_attrs": {
            "data-module": "qdes_autocomplete",
            "data-module-title": "title"
          },
          "form_include_blank_choice": true
        },
        {
          "field_name": "unit-of-measure",
